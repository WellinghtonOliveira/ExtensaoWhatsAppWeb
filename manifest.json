{
    "manifest_version": 2,
    "version": "0.1",
    "name": "WhatsApp Extensão",
    "content_scripts": [
        {
            "matches": [
                "https://web.whatsapp.com/"
            ],
            "css": [
                "styles.css"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "description": "Extensão para automoção de mensagens",
    "author": "Wellinghton O.",
    "browser_action": {
        "default_title": "Whats",
        "default_popup": "index.html"
    }    
}