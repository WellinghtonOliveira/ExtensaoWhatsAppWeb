{
    "manifest_version": 3,
    "name": "WhatsApp Message Sender",
    "version": "1.0",
    "description": "Enviar mensagens no WhatsApp Web com um clique",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "index.html"
    },
    "host_permissions": [
      "https://web.whatsapp.com/*"
    ]
  }
  